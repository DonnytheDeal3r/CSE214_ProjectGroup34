public class Shooter extends DefaultCritter {

    private static final double x0 = 400;
    private static final double y0 = 100;

    public Shooter() {
        super(x0, y0);
    }

    @Override
    public void update() {
        super.update();
    }

    @Override
    public void draw() {
        int radius = 10;
        StdDraw.setPenColor(StdDraw.BLUE);
        StdDraw.filledCircle(x, y, radius);
    }

}